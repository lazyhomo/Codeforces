#include <iostream>
#include <string>
using namespace std;

bool isPrime(long long num) {
    if (num <= 1) return false;
    if (num == 2) return true;
    if (num % 2 == 0) return false;

    for (long long i = 3; i * i <= num; i += 2) {
        if (num % i == 0) return false;
    }
    return true;
}

int main() {
    int t;
    cin >> t;

    while (t--) {
        int x, k;
        cin >> x >> k;

        string s = to_string(x);
        string out;
        for (int i = 0; i < k; i++) out += s;

       
        if (out.size() > 18) {
            cout << "NO\n";
            continue;
        }

        long long num = stoll(out);

        if (isPrime(num)) cout << "YES\n";
        else cout << "NO\n";
    }
    return 0;
}
