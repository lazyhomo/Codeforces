#include <iostream>
using namespace std;

int main() {
    long long n, m, a;
    cin >> n >> m >> a;

    long long tiles;

    if (n % a == 0 && m % a == 0) {
        tiles = (n / a) * (m / a);
    } else if (n > a && m > a) {
        tiles = (n / a + 1) * (m / a + 1);
    } else if (n > a && m <= a) {
        tiles = (n / a + 1);
    } else if (m > a && n <= a) {
        tiles = (m / a + 1);
    } else {
        tiles = 1;
    }

    cout << tiles;
    return 0;
}
